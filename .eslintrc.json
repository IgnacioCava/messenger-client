{
  "env": {
    "browser": true,
    "es2022": true
  },
  "parser":  "@typescript-eslint/parser",
  "extends": ["plugin:@next/next/recommended", "plugin:prettier/recommended", "plugin:@typescript-eslint/recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": ["off", { "singleQuote": true }]
  }
}
